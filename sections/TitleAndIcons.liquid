<div class="tai-container">
    <div class="tai-txt-area">
        <h2>{{ section.settings.tai_heading }}</h2>
        <p>{{ section.settings.tai_txt }}</p>
    </div>
    <div class="tai-icons-area">
        <div class="tai-f-3-icons">
            <div class="single-icon">
                <img src="https://cdn.shopify.com/s/files/1/0287/3828/9738/files/liposome_2.svg?v=1600120985" alt="">
                <p>{{ section.settings.tai_1_txt }}</p>
            </div>
            <div class="single-icon">
                <img src="https://cdn.shopify.com/s/files/1/0287/3828/9738/files/Vit_C_Collagen_Icon.svg?v=1600121563" alt="">
                <p>{{ section.settings.tai_2_txt }}</p>
            </div>
            <div class="single-icon">
                <img src="https://cdn.shopify.com/s/files/1/0287/3828/9738/files/Vit_C_Heart_icon.svg?v=1600121854" alt="">
                <p>{{ section.settings.tai_3_txt }}</p>
            </div>
        </div>
        <div class="tai-s-3-icons">
            <div class="single-icon">
                <img src="https://cdn.shopify.com/s/files/1/0287/3828/9738/files/Vit_C_Immune_system_icon.svg?v=1600121880" alt="">
                <p>{{ section.settings.tai_4_txt }}</p>
            </div>
            <div class="single-icon">
                <img src="https://cdn.shopify.com/s/files/1/0287/3828/9738/files/Vit_C_Energy_icon.svg?v=1600121901" alt="">
                <p>{{ section.settings.tai_5_txt }}</p>
            </div>
            <div class="single-icon">
                <img src="https://cdn.shopify.com/s/files/1/0287/3828/9738/files/Vit_C_Tired_icon.svg?v=1600121929" alt="">
                <p>{{ section.settings.tai_6_txt }}</p>
            </div>
        </div>
    </div>
</div>














{% schema %}
{
"name": "Title and Icons",
"settings": [
{
"type": "text",
"id": "tai_heading",
"label": "Heading",
"default": "Title"
},
{
"type": "textarea",
"id": "tai_txt",
"label": "Text"
},
{
"type": "image_picker",
"id": "tai_1_img",
"label": "First icon",
"info": "60px height recommended"
},
