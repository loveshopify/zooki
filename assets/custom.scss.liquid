@media (max-width: 1024px) {

  #theme-ajax-cart.is-mini-cart {
  }

  .is-relative {
    z-index: 9;
  }

}

#main_overlay {
  background: #1a1a1a;
  width: 100%;
  height: 100%;
  opacity: .8;
  position: fixed;
  top: 102px;
  opacity: 0;
  transition: opacity .3s ease;
  z-index: -1;
}

body.show-overlay {
  #main_overlay {
    display: block;
    opacity: .8;
    transition: opacity .3s ease;
  }
}


@media (max-width: 1024px) {

  .hide--mobile {
    display: none !important;
  }

  #mobile-header {
    z-index: 9999;
    .mobile-header__open-menu .icon {
      margin-left: 5px;
      @media (max-width: 1024px) {
        margin-left: 0px;
      }
    }
    .mobile-dropdown {
      @media (max-width: 1024px) {
        padding-left: 0px;
        padding-right: 0px;
        width: 18px;
      }
    }
    .mobile-header__content {
      height: 70px;
    }
    .mobile-menu-link.parent_link {
      color: #000;
    }
    .mobile-menu__submenu.has-dropdown {
      border: none;
      margin-left: 0px;
    }
    .has-submenu label.submenu__label {
      border-bottom: 1px solid #e2e8f0;
      a {
        font-family: "AvenirLTPro-Black";
        padding-top: 30px;
        padding-bottom: 30px;
      }
    }
    .mobile-menu__item.menu-level-1 {
      .icon {
        position: absolute;
        right: 6px;
      }
    }
    .mobile-icons {
      padding-right: 0px;
      [data-toggle-cart] {
        padding-left: 0px !important;
        padding-right: 0px !important;
        transform: unset !important;
      }
    }
    .sub_menu.menu-level-2 {
      &:first-child {
        border-top: 3px solid #000;
      }
      border-bottom: 1px solid #e2e8f0;
      display: flex;
      a {
        position: relative;
        width: 100%;
        &.custom--image {
          padding-top: 0 !important;
          padding-bottom: 0 !important;
          padding-left: 0 !important;
          img {
            height: 50px;
            width: 60px;
            object-fit: cover;
          }
        }
        .icon {
          position: absolute;
          right: 6px;
        }
      }
    }

    [data-ajax-cart-trigger] {
      padding: 0px;
      & > a.header__link {
        display: flex;
        align-items: center;
        height: 52px;
        padding-left: 20px;
        padding-right: 15px;
        transform: translateX(10px);
        &.opened {
          background: #eaeaea;
        }
      }
    }

    #theme-ajax-cart {
      .ajax-cart__close-icon {
        display: none;
      }
      .ajax-cart__form {
        background: #eaeaea;
      }
      .ajax-cart__button-wrapper {
        .ajax-cart__cart-link {
          display: none;
        }
      }
      .ajax-cart__cart-title {
        font-size: 24px;
        font-weight: normal;
        text-align: left;
        padding-top: 30px;
      }
      .ajax-cart__product {
        background: #fff;
        border-radius: 10px;
        margin-left: 20px;
        margin-right: 20px;
        .close {
          background: transparent;
        }
        .ajax-cart__price {
          span {
            font-size: 12px;
          }
        }
      }
      .ajax-cart__cart-count {
        box-shadow: 0 0 10px 0 rgba(0, 0, 0, 0.25);
        position: relative;
        z-index: 10;
      }
      .ajax-cart__details-wrapper {
        box-shadow: 0 0 10px 0 rgba(0, 0, 0, 0.25);
        padding-bottom: 30px;
        position: relative;
        z-index: 10;
      }
      .ajax-cart__button-wrapper button {
        height: 45px;
        margin-top: 0px;
      }
      .ajax-cart__subtotal {
        .ajax-cart__row-description {
          font-size: 16px;
        }
        span.money {
          font-size: 24px;
        }
      }
    }
    .ajax-cart__list {
      height: calc(100vh - 280px);
      overflow: auto;
      padding-top: 30px;
      padding-bottom: 30px;
    }
  }

  .mobile-menu-tabs {
    border-top: 1px solid #1d1d1d;
    z-index: 99999;
    .tab-contents {
      [data-tab-content].is_active {
        height: calc(100vh - 140px);
        overflow: auto;
        z-index: 99999;
        padding-bottom: 85px;
        &.data-tab-content-currency {
          padding-top: 24px;
        }
        &.data-tab-content-account {
          padding-top: 44px;
        }
        & > div {
          padding-top: 0;
        }
      }
    }
    .tab-header {
      display: flex;
      align-items: center;
      justify-content: center;
      height: 70px;
      max-width: calc(100% / 3);
      min-width: calc(100% / 3);
      cursor: pointer;
      &:first-child svg {
        width: 25px;
      }
      svg {
        width: 20px;
      }
      &:first-child {
        border-right: 1px solid #e4e4e4;
      }
      &:last-child {
        border-left: 1px solid #e4e4e4;
      }
      &.is_active {
        background: #eaeaea;
      }
      img {
        transform: unset;
      }
    }
  }
}

.tab-content.data-tab-content-cart.is_active {
  @media (min-width: 1024px) {
    & + .tab-content-overlay {
      display: none !important;
    }
  }
}
.tab-contents {
  background: #fff;
  position: absolute;
  width: 100%;
  overflow: hidden;
  z-index: 9;
  @media (max-width: 1024px) and (min-width: 800px) {
    max-height: calc(100vh - 172px);
  }
  @include mobile {
  }
}
[data-tab-content] {
  background: #eaeaea;
  display: none;
  padding-left: 20px;
  padding-right: 20px;
  &.is_active {
    display: block;
    @include mobile {
      height: 572px;
    }
  }
  &.data-tab-content-currency {
    padding-left: 24px;
    padding-right: 24px;
    .country-selection {
      padding-top: 12px;
      padding-bottom: 30px;
      li.active img{
        transform: unset !important;     
      }
      a {
        background: #FFF;
        border-radius: 10px;
        display: flex;
        height: 70px;
        margin-top: 12px;
        margin-bottom: 12px;
        padding: 10px;
        padding-top: 13px;
        .icon_wrapper {
          margin-right: 10px;
        }
        .info_wrapper {
          flex: 1;
          padding-right: 5px;
          .title {
            color: #1a1a1a;
            font-size: 14px;
            margin-bottom: 10px;
            & + div {
              display: flex;
              justify-content: space-between;
            }
          }
          span {
            color: #8c8c8c;
            font-weight: normal;
            font-size: 14px;
          }
        }

      }
      li {
        background: transparent;
        list-style: none;
        &.is_active a {
          background: #000;
          .title {
            color: #fff;
          }
          .icon_wrapper {
            img {
              border: 1px solid #FFF;
            }
          }
        }
      }
    }
  }
  &.data-tab-content-search {
    padding-bottom: 40px;
    form {
      padding: 45px 0 !important;
    }
    input.input {
      background: transparent;
      border-bottom: 3px solid #FFF;
      height: unset;
      padding-top: 10px;
      padding-bottom: 10px;
      padding-left: 0px;
      padding-right: 0px;
      font-size: 24px !important;
      color: #1a1a1a;
    }
    input[type="submit"] {
      background: #000;
      border-radius: 5px;
      color: #FFF;
      font-size: 16px;
      font-weight: 600;
      font-family: "Avenir", sans-serif;
      height: 45px;
      margin-top: 15px;
      width: 100%;
    }
  }
  &.data-tab-content-account {
    .form-wrapper {
      padding-top: 30px;
      &.is-hidden {
        display: none;
      }
      .form-title {
        color: #000;
        font-size: 24px;
        font-weight: normal;
        margin-bottom: 20px;
      }
      .field {
        label {
          color: #8c8c8c;
        }
        input {
          border-radius: 5px;
          height: 55px;
        }
      }
      .field:not(:last-child) {
        margin-top: 20px;
      }
      input[type='submit'] {
        height: 45px;
        margin-top: 30px;
        width: 100%;
      }
      .form-footer {
        background: #f3f0ed;
        font-size: 24px;
        font-weight: normal;
        margin-left: -20px;
        margin-top: 90px;
        width: calc(100% + 40px);
        padding: 50px 20px;
        a {
          color: #ec7236;
          display: block;
          font-size: 16px;
          font-weight: normal;
          margin-top: 30px;
        }
      }

      &.form-signin {
        padding-bottom: 30px;
        .js-recover-password {
          color: #ec7236;
          display: inline-block;
          font-weight: normal;
          margin-top: 5px;
        }
        .has-padding-top {
          font-weight: normal;
        }
        a {
          color: #ec7236;
        }
      }

      &.form-reset-psd {
        padding-bottom: 45px;
        .form-subtitle {
          font-weight: normal;
        }
        input[type='submit'] {
          background: #000;
          border-radius: 5px;
          color: #fff;
          font-weight: 700;
        }
        a {
          color: #ec7236;
          display: inline-block;
          font-weight: normal;
          margin-top: 15px;
        }
      }
    }
  }
}

@media (max-width: 1024px){
