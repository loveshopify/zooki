// ============================================== Ajax Cart =============================================== //

#theme-ajax-cart {
  overflow: hidden !important;
    section.ajax-cart--mini-cart {
      height: 100%;
      & > .card {
        height: 100%;
        #ajax-cart__content {
          height: 100%;
          form {
            height: 100%;
          }
        }
      }
    }

  section.ajax-cart--mini-cart > .card #ajax-cart__content form .ajax-cart__list {
      height: calc(100% - 233px);
    @media (max-width: 768px) {
      height: calc(100% - 229px);
    }
  }

  #ajax-cart__content {
    .ajax-cart__empty-cart-message {
      background: #f7f7f7;
      height: calc(100vh - 122px);
      @media (max-width: 768px) {
        height: calc(100vh - 52px);
      }
    }
    form.ajax-cart__form {
      .ajax-cart__list {
        # height: calc(100vh - 335px);
        margin-bottom: 0px;
        margin-top: 0px;
        overflow: auto;
        .ajax-cart__product:first-child {
          margin-top: 1rem;
        }
        @media (max-width: 798px) {
          height: calc(100vh - 280px);
        }
        @include mobile {
          height: calc(100vh - 280px);
        }
      }
    }
  }
}

