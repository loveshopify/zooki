.active {
	background-color: #363636;
	border: none;
	outline: none !important;
	img {
		transition: 0.4s;
		transform: rotate(180deg);
	}
}

.faq-container {
	background-color: #363636;
  	margin-top: 60px;
	.inner-faq-container {
		padding: 50px 60px 60px 60px;
		h4 {
			font-family: 'AvenirLTPro-Black';
			font-size: 28px;
			font-weight: 900;
			font-stretch: normal;
			font-style: normal;
			line-height: normal;
			letter-spacing: normal;
			text-align: center;
			margin: 0 auto;
			color: #ffffff;
			margin-bottom: 50px;
		}
		.accordion {
			background-color: #363636;
			cursor: pointer;
			padding: 0px;
			padding-top: 18px;
			padding-right: 12px;
			padding-bottom: 18px;
			padding-left: 12px;
			width: 100%;
			text-align: left;
			border: none;
			outline: none;
			transition: 0.4s;
			font-family: 'AvenirLTPro-Roman';
			font-size: 18px;
			font-weight: normal;
			font-stretch: normal;
			font-style: normal;
			line-height: normal;
			letter-spacing: normal;
			color: #ffffff;
			text-align: left;
			// border-bottom: 1px solid #ffffff;
			display: flex;
			justify-content: space-between;
			img {
				transition: 0.4s;
			}
			&:focus {
				outline: 0px auto -webkit-focus-ring-color !important;
			}
			&:hover {
				background-color: #363636;
				border: none;
				outline: none !important;
			}
		}
		.panel {
			padding: 0 12px;
			background-color: #363636;
			color: #fff;
			max-height: 0;
			overflow: hidden;
			transition: max-height 0.2s ease-out;
			border-bottom: 1px solid #8c8c8c;
			.faq-answer {
				font-family: 'AvenirLTPro-Roman';
				font-size: 16px;
				font-weight: normal;
				font-stretch: normal;
				font-style: normal;
				line-height: normal;
				letter-spacing: normal;
				color: #c3c3c3;
              	padding-bottom: 18px;
              	padding-right: 48px;
			}
		}
	}
}

.faq-arrow {
  margin-left: 12px;
}
