// Modal

.ing-modal {
	position: fixed;
	left: 0;
	top: 0;
	width: 100%;
	height: 100%;
	background-color: rgba(0, 0, 0, 0.5);
	opacity: 0;
	visibility: hidden;
	transform: scale(1.1);
	transition: visibility 0s linear 0.25s, opacity 0.25s 0s, transform 0.25s;
}
.ing-modal-content {
	position: absolute;
	top: 50%;
	left: 50%;
	transform: translate(-50%, -50%);
	background-color: white;
	padding: 48px 36px;
	width: 449px;
	position: relative;
	z-index: 999;
	@media (max-width: 768px){
		max-width: 340px;
	}
	h2 {
		font-family: 'AvenirLTPro-Black';
		font-size: 28px;
		font-weight: 900;
		font-stretch: normal;
		font-style: normal;
		line-height: normal;
		letter-spacing: normal;
		color: #1a1a1a;
		margin-bottom: 24px;
	}
	.ing-content {
		font-family: 'AvenirLTPro-Roman';
		font-size: 14px;
		font-weight: normal;
		font-stretch: normal;
		font-style: normal;
		line-height: normal;
		letter-spacing: normal;
		color: #1a1a1a;
	}
}
.ing-close-button {
	float: right;
	width: 60px;
	height: 60px;
	line-height: 3.5rem;
	text-align: center;
	cursor: pointer;
	background-color: #1a1a1a;
	font-size: 35px;
	position: absolute;
	right: 0;
	top: -60px;
	color: #fff;
}
.ing-close-button:hover {
	background-color: darkgray;
}

.ing-show-modal {
	z-index: 999;
	opacity: 1;
	visibility: visible;
	transform: scale(1.0);
	transition: visibility 0s linear 0s, opacity 0.25s 0s, transform 0.25s;
}

.nut-modal {
	position: fixed;
	left: 0;
	top: 0;
	width: 100%;
	height: 100%;
	background-color: rgba(0, 0, 0, 0.5);
	opacity: 0;
	visibility: hidden;
	transform: scale(1.1);
	transition: visibility 0s linear 0.25s, opacity 0.25s 0s, transform 0.25s;
}
.nut-modal-content {
	position: absolute;
	top: 50%;
	left: 50%;
	transform: translate(-50%, -50%);
	background-color: white;
	padding: 48px 36px;
	width: 449px;
	position: relative;
	z-index: 999;
	@media (max-width: 768px){
		max-width: 340px;
	}
	h2 {
		font-family: 'AvenirLTPro-Black';
		font-size: 28px;
		font-weight: 900;
		font-stretch: normal;
		font-style: normal;
		line-height: normal;
		letter-spacing: normal;
		color: #1a1a1a;
		margin-bottom: 24px;
	}
	.nut-content {
		font-family: 'AvenirLTPro-Roman';
		font-size: 14px;
		font-weight: normal;
		font-stretch: normal;
		font-style: normal;
		line-height: normal;
		letter-spacing: normal;
		color: #1a1a1a;
		tbody {
			tr {
				td {
					width: 184px;
					font-family: 'AvenirLTPro-Roman';
					font-size: 14px;
					font-weight: normal;
					font-stretch: normal;
					font-style: normal;
					line-height: normal;
					letter-spacing: normal;
					color: #1a1a1a;
					margin-right: 12px;
				}
			}
		}
	}
}
.nut-close-button {
	float: right;
	width: 60px;
	height: 60px;
	line-height: 3.5rem;
	text-align: center;
	cursor: pointer;
	background-color: #1a1a1a;
	font-size: 35px;
	position: absolute;
	right: 0;
	top: -60px;
	color: #fff;
}
.nut-close-button:hover {
	background-color: darkgray;
}

.nut-show-modal {
	z-index: 999;
	opacity: 1;
	visibility: visible;
	transform: scale(1.0);
	transition: visibility 0s linear 0s, opacity 0.25s 0s, transform 0.25s;
}
