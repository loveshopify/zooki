{% comment %}
  To edit the contents of this file, look inside the `sections` folder for the file name that comes directly after `section`. Eg. {% section 'product__main' %} or {% section 'page__banner' %}
{% endcomment %}

<main id="template-{{ template | replace: '.', '-' | handle }}" class="shopify-section blog-page" data-check-for-order="true">
  {% section 'blog__banner' %}
  <section class="section__wrapper
                  section
                  ">
    <div class="container">
      {% section 'blog__sidebar' %}
      {% section 'blog__main' %}
    </div>
  </section>
</main>